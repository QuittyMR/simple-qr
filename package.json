{
  "name": "simple-qr",
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "tailwind": "npx tailwindcss -i public/css/tailwind.css -o public/css/app.css -w",
    "build": "rollup -c",
    "dev": "concurrently \"rollup -c -w\" \"npm run tailwind\"",
    "start": "sirv public --no-clear --single --dev"
  },
  "devDependencies": {
    "@capacitor/cli": "^3.4.3",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@smui/snackbar": "^6.0.0-beta.16",
    "autoprefixer": "^10.4.4",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sirv-cli": "^2.0.0",
    "svelte": "^3.0.0",
    "tailwindcss": "^3.0.24"
  },
  "dependencies": {
    "@capacitor/android": "^3.4.3",
    "@capacitor/core": "^3.4.3",
    "qr-scanner": "^1.4.1"
  }
}
